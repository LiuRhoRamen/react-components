{
    "ActionList": {
        "path": "ActionList/ActionList.tsx",
        "name": "ActionList",
        "info": {
            "description": "",
            "displayName": "ActionList",
            "methods": [],
            "props": {
                "actionList": {
                    "required": true,
                    "tsType": { "name": "Array", "elements": [{ "name": "ActionInfo" }], "raw": "ActionInfo[]" },
                    "description": { "description": "操作列表", "tags": [] }
                },
                "exposeCount": {
                    "required": false,
                    "tsType": { "name": "number" },
                    "description": { "description": "暴露的操作数量", "tags": [] }
                },
                "autoAdjustment": {
                    "required": false,
                    "tsType": { "name": "boolean" },
                    "description": { "description": "是否自动按照宽度调整展示数量", "tags": [] }
                },
                "size": {
                    "required": false,
                    "tsType": { "name": "Size" },
                    "description": { "description": "控件尺寸", "tags": [] }
                },
                "smart": {
                    "required": false,
                    "tsType": { "name": "boolean" },
                    "description": {
                        "description": "操作数量等于 exposeCount+1 时是否直接显示按钮而不是显示下拉菜单",
                        "tags": []
                    }
                },
                "buttonStyleType": {
                    "required": false,
                    "tsType": { "name": "ButtonProps['styleType']", "raw": "ButtonProps['styleType']" },
                    "description": { "description": "按钮的默认样式类别，参考 Button 的 styleType", "tags": [] }
                },
                "dropdownButton": {
                    "required": false,
                    "tsType": {
                        "name": "union",
                        "raw": "ReactNode | ButtonProps",
                        "elements": [{ "name": "ReactNode" }, { "name": "ButtonProps" }]
                    },
                    "description": { "description": "自定义更多按钮内容，也可通过传入 object 来定义 props", "tags": [] }
                },
                "popoverProps": {
                    "required": false,
                    "tsType": { "name": "any" },
                    "description": { "description": "弹出层的 popover props", "tags": [] }
                }
            }
        }
    }
}
